    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/unsw_logo_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MainTheme">
        <activity android:name=".View.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.MainActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.StudentListActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.BaseActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AddStudentActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.TutorialListActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AssessmentListActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AddAssessmentActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.CompetencyListActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AddCompetencyActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.WeeksListActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AddWeekDetailsActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.DummyActivity" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.TutorialStudents" />
        <activity android:name="au.com.owenwalsh.capabilityconnect.View.AddTutorialActivity" />
    </application>

</manifest>